{
    "name": "webmaniacs.net/lib-uri",
    "description": "Represents a Uniform Resource Identifier (URI), Uniform Resource Locator (URL) and File Resource references. RFC 2396, RFC 2732 compatible",
    "license": "MIT",
    "authors": [
        {
            "name": "Stas Dovgodko",
            "email": "stas.dovgodko@webmaniacs.net"
        }
    ],
    "keywords": [
        "URI",
        "http",
        "URL",
        "RFC-2396",
        "RFC-2732"
    ],

    "autoload": {
        "psr-4": {
            "wmlib\\uri\\": "lib/"
        }
    },

    "require-dev": {
